	"切换画面":{
		"scope": "ruby",
		"prefix": "swcreen(",
		"body": [
			":${1:符号}: = ${2:画面号}",
			"swscreen(0, 100 + :${1:符号}(1):, :${1:符号}(2):, 0, 99, 0)",
			"$0"  
		],
		"description": "脚本切换画面"
	},


	"ARG[]":{
		"scope": "ruby",
		"prefix": "if ARG[",
		"body": [
			"if ARG[${1:0}] == ${2:节号} then",
				"$0",
			"end"
		],
		"description": "ARG[] 脚本分节"
	}
